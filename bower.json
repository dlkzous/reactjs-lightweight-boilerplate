{
  "name": "reactjs-lightweight-boilerplate",
  "description": "Lightweight ReactJS boilerplate application that uses the flux architecture. Set up with eslint and jsdoc.",
  "main": "src/js/App.js",
  "authors": [
    "Kushal L. DSouza <kushlyn@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "reactjs",
    "flux",
    "eslint",
    "jsdoc",
    "es6",
    "webpack",
    "http",
    "lightweight",
    "boilerplate",
    "materialisecss"
  ],
  "homepage": "https://github.com/kushaldsouza/reactjs-lightweight-boilerplate",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests",
    ".DS_STORE",
    "node_modules,"
    "*~",
  "*.pyc",
  "static",
  ".grunt",
  "_SpecRunner.html",
  "__benchmarks__",
    "build/",
    "lint/",
    "docs/",
    "coverage/",
    ".module-cache",
    "*.gem",
    "docs/.bundle",
    "docs/code",
    "docs/_site",
    "docs/.sass-cache",
    "docs/js/*",
    "docs/downloads",
    "docs/vendor/bundle",
    "examples/shared/*.js",
  "test/the-files-to-test.generated.js",
  "*.log*",
  "chrome-user-data",
  "*.sublime-project",
  "*.sublime-workspace",
  ".idea",
  "*.iml",
  ".vscode",
  ]
}
